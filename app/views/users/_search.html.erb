<% if @address.count > 0 %>
<h1>Search Results</h1>
<table class="table table-hover">
<thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>District</th>
      <th>State</th>
    </tr>
  </thead>
 <tbody>
   <% @address.each  do |address|%>
      <tr>
        <td><%= address.user.name%></td>
        <td><%= address.city %></td>
        <td><%= address.distt %></td>
        <td><%= address.state %></td>

 </tr>
    <% end %>
  </tbody>
</table>
<%else %>
<h3>No record found </h3>
<%end%>


